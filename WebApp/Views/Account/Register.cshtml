@model AccountRegisterViewModel
@{
    ViewData["Title"] = "Register new account";
}
<section class="wrapper">
    <header>
    </header>
    <div class="center-content">
        <div class="container">
            <form id="account-registration">
                <div>
                    <label asp-for="FirstName" class="form-label"></label>
                    <input asp-for="FirstName" />
                    <span asp-validation-for="FirstName"></span>
                </div>
                <div>
                    <label asp-for="LastName" class="form-label"></label>
                    <input asp-for="LastName" />
                    <span asp-validation-for="LastName"></span>
                </div>
                <div>
                    <label asp-for="StreetName" class="form-label"></label>
                    <input asp-for="StreetName" />
                    <span asp-validation-for="StreetName"></span>
                </div>
            </form>
        </div>
    </div>
    <footer>
    </footer>
</section>